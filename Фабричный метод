class Animal:
    def make_sound(self):
         pass

class Lion(Animal):
    def make_sound(self):
         return "Roar!"

class Monkey(Animal):
    def make_sound(self):
         return "Ooh oo aa aa!"

class Elephant(Animal):
    def make_sound(self):
         return "Trumpet!"

class AnimalFactory:
    def create_animal(self):
        pass

class LionFactory(AnimalFactory):
    def create_animal(self):
        return Lion()

class MonkeyFactory(AnimalFactory):
    def create_animal(self):
        return Monkey()

class ElephantFactory(AnimalFactory):
    def create_animal(self):
        return Elephant()
